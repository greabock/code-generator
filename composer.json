{
    "name": "railt/code-generator",
    "license": "MIT",
    "type": "library",
    "homepage": "http://railt.org",
    "description": "The Railt Framework implementation of GraphQL code genertor",
    "keywords": [
        "generator",
        "code",
        "graphql",
        "scaffolding"
    ],
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/code-generator"
    },
    "require": {
        "php": ">=7.4",
        "graphql/type-system-contracts": "^14.0",
        "railt/common": "^2.0",
        "railt/config": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Railt\\CodeGenerator\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\CodeGenerator\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "1.3.x": "1.3.x-dev",
            "1.4.x": "1.4.x-dev",
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
